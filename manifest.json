{
  "description": "Downloads run data from Garmin connect detail page",
  "manifest_version": 2,
  "name": "garmin-run-downloader",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
 
  "browser_action": {
    "default_icon": {
      "16": "icons/grd16.png",
      "32": "icons/grd32.png",
      "48": "icons/grd48.png",
      "64": "icons/grd64.png"
    }
  }
}
